<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iyf.salesledger.dao.EmpResumeDao">
	<insert id="insert" parameterType="com.iyf.salesledger.model.EmpResume">
		INSERT INTO t_emp_resume (resume_id, emp_pool_id, file_name, file_regdate)
		VALUES (default , #{emp_pool_id}, #{file_name}, now())
	</insert>
	<select id="maxKeySelect" resultType="Long">
		select max(emp_pool_id) emp_pool_id from t_emp_pool
	</select>
	<select id="selectList" parameterType="Long" resultType="com.iyf.salesledger.model.EmpResume">
		select * from t_emp_resume where emp_pool_id = #{emp_pool_id}
	</select>
	<select id="select" parameterType="Long" resultType="com.iyf.salesledger.model.EmpResume">
		select * from t_emp_resume where resume_id = #{resume_id}
	</select>
</mapper>